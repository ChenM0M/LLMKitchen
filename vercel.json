{
    "rewrites": [
        {
            "source": "/api/text",
            "destination": "/api/text.ts"
        },
        {
            "source": "/api/image",
            "destination": "/api/image.ts"
        },
        {
            "source": "/api/elysiver/:path*",
            "destination": "https://elysiver.h-e.top/:path*"
        },
        {
            "source": "/api/newapi/:path*",
            "destination": "https://newapi.netlib.re/:path*"
        },
        {
            "source": "/api/siliconflow/:path*",
            "destination": "https://api.siliconflow.cn/:path*"
        }
    ]
}